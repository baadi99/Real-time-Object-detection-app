                     </Menu>
                  </menus>
               </MenuBar>
            </HBox>
         </left>
         <right>
            <HBox>
               <Button fx:id="fullScreen" onMouseClicked="#enterFullScreen" prefHeight="25.0" prefWidth="40.0">
                  <graphic>
                     <ImageView fx:id="enterExitFullScreenIcon" fitHeight="15" fitWidth="15" pickOnBounds="true" preserveRatio="true">
                        <Image url="@Icones/fullScreenIcon.png" />
                     </ImageView>
                  </graphic>
                  <cursor>
                     <Cursor fx:constant="HAND" />
                  </cursor>
               </Button>
               <Button fx:id="exitBtn" onMouseClicked="#exit" prefHeight="25.0" prefWidth="40.0">
                  <graphic>
                     <ImageView fitHeight="15" fitWidth="15" pickOnBounds="true" preserveRatio="true">
                        <Image url="@Icones/exitIcon.png" />
                     </ImageView>
                  </graphic>
                  <cursor>
                     <Cursor fx:constant="HAND" />
                  </cursor>
               </Button>
            </HBox>
         </right>
      </BorderPane>
   </top>
   <center>
      <HBox alignment="center" style="-fx-background-color:#000;">
         <ImageView fx:id="videoContainer" />
      </HBox>
   </center>
   <bottom>
      <HBox alignment="center">
         <padding>
            <Insets bottom="5" left="25" right="25" top="5" />
         </padding>
         <Button fx:id="playBtn" alignment="center" onAction="#start" text="Play">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor>
            <graphic>
               <ImageView fx:id="playPauseIcon" fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                  <Image url="@Icones/playIcon.png" />
               </ImageView>
            </graphic>
         </Button>
      </HBox>
   </bottom>
</BorderPane>
