                           <ImageView fitHeight="15" fitWidth="15" pickOnBounds="true" preserveRatio="true">
                              <Image url="@Icones/settingsIcon.png" />
                           </ImageView>
                        </graphic>
                        <Menu text="Detect : ">
                           <items>
                              <RadioMenuItem fx:id="all" text="All">
                              <toggleGroup>
                                 <ToggleGroup fx:id="objectClasses"/>
                              </toggleGroup>
                              </RadioMenuItem>
