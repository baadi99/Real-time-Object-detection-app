<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="container" stylesheets="@Styles/homePageStyles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.videoController">
   <top>
      <BorderPane prefHeight="26.0">
         <left>
            <HBox>
               <Button fx:id="home" onMouseClicked="#goHome" prefHeight="25.0" prefWidth="72.0" text="Home">
